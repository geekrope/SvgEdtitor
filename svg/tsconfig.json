{
    "compilerOptions":
    {
        "strict": true,
        "diagnostics": false,
        "extendedDiagnostics": false,
        "incremental": true,
        "alwaysStrict": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "strictPropertyInitialization": true,
        "noEmitOnError": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "noErrorTruncation": true,
        "allowJs": false,
        "removeComments": true,
        "sourceMap": true,
        "skipLibCheck": true,
        "target": "es5",
        "outDir": "./script",
        "outFile": "./script/main.js",
        "module": "System"
    },
    "exclude":
    [
        "node_modules",
        "wwwroot"
    ]
}
